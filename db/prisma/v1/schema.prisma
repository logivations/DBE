model data_adwords {
  id                  Int    @id @default(autoincrement())
  country             String @db.VarChar(30)
  firm_region         String @db.VarChar(100)
  locationCoordinates String @db.VarChar(100)
  date                String @db.VarChar(20)
  come_from           String @db.VarChar(250)
  come_to             String @db.VarChar(50)
  ip                  String @db.VarChar(70)
  browser             String @db.VarChar(50)
}

model elearning_users {
  reg_name  String? @db.VarChar(30)
  company   String? @db.VarChar(30)
  position  String? @db.VarChar(50)
  email     String? @db.VarChar(50)
  phone     String? @db.VarChar(20)
  pass_word String? @db.VarChar(512)
  confirm   Int     @default(0)
  id        Int     @id @default(autoincrement()) @db.UnsignedInt
}

