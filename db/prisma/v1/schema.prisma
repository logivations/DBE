generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:11111111@localhost:3306/lv_home"
}

model anmeldeformulare {
  ID           Int      @id @default(autoincrement()) @db.UnsignedInt
  name         String   @db.VarChar(30)
  header       String   @db.Text
  subheader    String   @db.Text
  introduction String   @db.Text
  bullets      String   @db.Text
  tick1        String   @db.Text
  tick2        String   @db.Text
  tick3        String   @db.Text
  agendalink   String   @db.Text
  email_text   String   @db.Text
  enter_time   Boolean?
}

model browserdetail {
  id     Int     @id @default(autoincrement()) @db.UnsignedSmallInt
  cookie String? @db.VarChar(1000)
  owner  String? @db.VarChar(100)

  @@index([cookie(length: 30)], map: "asdf")
}

model cal_entries {
  id          Int      @id @default(autoincrement()) @db.UnsignedSmallInt
  text        String   @db.Text
  datum       DateTime @db.Date
  lastchanged DateTime @db.DateTime(0)
  person      String   @db.Text
}

model circle {
  id      Int      @id @default(autoincrement()) @db.UnsignedSmallInt
  name_id Int      @db.UnsignedSmallInt
  score   Int      @db.UnsignedInt
  anzahl  Int      @db.UnsignedSmallInt
  datum   DateTime @db.DateTime(0)
  browser String   @db.Text
}

model cookies_names {
  id     Int     @id @default(autoincrement()) @db.UnsignedInt
  cookie String  @db.VarChar(1000)
  owner  String? @db.VarChar(100)

  @@index([cookie(length: 10)], map: "ccc")
}

model cookies_names_adwords {
